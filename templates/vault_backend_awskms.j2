seal "awskms" {
  kms_key_id = "{{ vault_awskms_key_id }}"
{% if vault_awskms_region %}
  region     = "{{ vault_awskms_region }}"
{% endif %}
{% if vault_awskms_access_key %}
  access_key = "{{ vault_awskms_access_key }}"
{% endif %}
{% if vault_awskms_secret_key %}
  secret_key = "{{ vault_awskms_secret_key }}"
{% endif %}
{% if vault_awskms_session_token %}
  session_token = "{{ vault_awskms_session_token }}"
{% endif %}
{% if vault_awskms_endpoint %}
  endpoint   = "{{ vault_awskms_endpoint }}"
{% endif %}
}
